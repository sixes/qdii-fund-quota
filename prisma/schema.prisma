generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FundQuota {
  id           Int    @id @default(autoincrement())
  fund_company String
  fund_name    String
  share_class  String
  fund_code    String @db.VarChar(20)
  quota        Int
  currency     String @db.Char(3)

  @@map("fund_quota")
}
